{
  "name": "dictator-builder",
  "version": "0.0.19",
  "scripts": {
    "build": "mkdir -p lib && npm run generate-schema && npm run format && tsc -p .",
    "generate-schema": "npx typescript-json-schema src/types.d.ts DictatableConfig --required --noExtraProps --topRef -o src/schema.json && cp src/schema.json lib && npm run generate-markdown-schema",
    "generate-markdown-schema": "node generate-markdown-schema.js",
    "format": "prettier --write '**/*{.md,.js,.json,.json,.ts}'",
    "test": "npm run build && jest",
    "release": "npm version patch && npm run test && npm publish && git push --follow-tags"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^10.0.0",
    "babel-jest": "^25.4.0",
    "fs-extra": "^9.0.0",
    "jest": "^25.4.0",
    "json-schema-to-markdown": "^1.1.1",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "ts-node": "^8.9.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/glob": "^7.1.1",
    "chalk": "^4.0.0",
    "clear": "^0.1.0",
    "commander": "^5.0.0",
    "dir-compare": "^2.3.0",
    "figlet": "^1.3.0",
    "glob": "^7.1.6",
    "jsonpath": "^1.0.2",
    "jsonschema": "^1.2.6",
    "path": "^0.12.7",
    "pkg-up": "^3.1.0",
    "rimraf": "^3.0.2",
    "underscore": "^1.10.2"
  },
  "homepage": "https://github.com/tomasbjerre/dictator-builder",
  "repository": "git@github.com:tomasbjerre/dictator-builder.git",
  "files": [
    "lib"
  ],
  "author": "Tomas Bjerre",
  "keywords": [
    "dictator",
    "husky",
    "automation",
    "repository-config"
  ],
  "license": "Apache 2"
}
