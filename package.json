    "format": "prettier --write '{*.md,*.json,src/**/*.ts}'"
  },
  "devDependencies": {
    "@types/node": "^10.0.0",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "ts-node": "^8.9.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "clear": "^0.1.0",
    "commander": "^5.0.0",
    "figlet": "^1.3.0",
    "path": "^0.12.7",
    "pkg-up": "^3.1.0"
  }
}
